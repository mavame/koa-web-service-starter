  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/server/index.js",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Knex migrate",
      "program": "${workspaceFolder}/node_modules/.bin/knex",
      "console": "integratedTerminal",
      "args": [
        "migrate:latest"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Knex seed",
      "program": "${workspaceFolder}/node_modules/.bin/knex",
      "console": "integratedTerminal",
      "args": [
        "seed:run"
      ]
    },
  ]
}